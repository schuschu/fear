style "gedit-frame"
{	

xthickness = 3
	engine "pixmap"
	{
 		image
		{
			function = SHADOW
                	file = ""
		}
	}
}

widget_class "*Gedit*.<GtkFrame>*"     			style "gedit-frame"
widget_class "*Gedit*.<GtkScrolledWindow>"     		style "murrine-thin"
widget_class "*Gedit*.<GtkArrow>*"     			style:highest "default"
